  }
sga-pnum =
  
  ## (number) contains a number, written in any form. [3.5.3. ]
  element num {
    sga-pmacro.phraseSeq,
    sga-patt.global.attributes,
    sga-patt.ranging.attributes,
    
    ## indicates the type of numeric value.
    ## Suggested values include: 1] cardinal; 2] ordinal; 3] fraction; 4] percentage
    attribute type {
      
      ## absolute number, e.g. 21, 21.5
      "cardinal"
      | 
        ## ordinal number, e.g. 21st
        "ordinal"
      | 
        ## fraction, e.g. one half or three-quarters
        "fraction"
      | 
        ## a percentage
        "percentage"
      | xsd:Name
    }?,
    
    ## supplies the value of the number in standard form.
    attribute value { sga-pdata.numeric }?,
    empty
  }
sga-pmeasureGrp =
  
  ## (measure group) contains a group of dimensional specifications which relate to the same object, for example the height and width of a manuscript page. [10.3.4. ]
  element measureGrp {
    (text | sga-pmodel.gLike | sga-pmodel.measureLike)*,
    sga-patt.global.attributes,
    sga-patt.measurement.attributes,
    sga-patt.typed.attributes,
    empty
  }
sga-pdate =
  
  ## contains a date in any format. [3.5.4.  2.2.4.  2.5.  3.11.2.3.  15.2.3.  13.3.6. ]
  element date {
    (text | sga-pmodel.gLike | sga-pmodel.phrase | sga-pmodel.global)*,
    sga-patt.global.attributes,
    sga-patt.datable.attributes,
    sga-patt.editLike.attributes,
    sga-patt.typed.attributes,
    empty
  }
sga-pptr =
  
  ## (pointer) defines a pointer to another location. [3.6.  16.1. ]
  element ptr {
    empty
    >> sch:pattern [
         id = "ptr-constraint-ptrAtts"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:ptr"
           "\x{a}" ~
           "               "
           sch:report [
             test = "@target and @cRef"
             "Only one of the\x{a}" ~
             "attributes @target and @cRef may be supplied on "
             sch:name [ ]
             "."
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ],
    sga-patt.global.attributes,
    sga-patt.pointing.attributes,
    sga-patt.internetMedia.attributes,
    sga-patt.typed.attributes,
    sga-patt.cReferencing.attributes,
    empty
  }
sga-pref =
  
  ## (reference) defines a reference to another location, possibly modified by additional text or comment. [3.6.  16.1. ]
  element ref {
    sga-pmacro.paraContent
    >> sch:pattern [
         id = "ref-constraint-refAtts"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:ref"
           "\x{a}" ~
           "               "
           sch:report [
             test = "@target and @cRef"
             "Only one of the\x{a}" ~
             "	attributes @target' and @cRef' may be supplied on "
             sch:name [ ]
             "\x{a}" ~
             "               "
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ],
    sga-patt.global.attributes,
    sga-patt.pointing.attributes,
    sga-patt.internetMedia.attributes,
    sga-patt.typed.attributes,
    sga-patt.cReferencing.attributes,
    empty
  }
sga-plist =
  
  ## (list) contains any sequence of items organized as a list. [3.7. ]
  element list {
    ((sga-pmodel.divTop | sga-pmodel.global)*,
     ((sga-pitem, sga-pmodel.global*)+
      | (sga-pmodel.global*, sga-pitem, sga-pmodel.global*)+),
     (sga-pmodel.divBottom, sga-pmodel.global*)*),
    sga-patt.global.attributes,
    sga-patt.sortable.attributes,
    
    ## describes the form of the list.
    ## Suggested values include: 1] ordered; 2] bulleted; 3] simple; 4] gloss
    [ a:defaultValue = "simple" ]
    attribute type {
      
      ## list items are numbered or lettered. 
      "ordered"
      | 
        ## list items are marked with a bullet or other typographic device. 
        "bulleted"
      | 
        ## list items are not numbered or bulleted. 
        "simple"
      | 
        ## each list item glosses some term or concept, which is given by a label element preceding the list item.
        "gloss"
      | xsd:Name
    }?,
    empty
  }
sga-pitem =
  
  ## contains one component of a list. [3.7.  2.5. ]
  element item {
    sga-pmacro.specialPara,
    sga-patt.global.attributes,
    sga-patt.sortable.attributes,
    empty
  }
sga-pnote =
  
  ## contains a note or annotation. [3.8.1.  2.2.6.  3.11.2.7.  9.3.5.4. ]
  element note {
    sga-pmacro.specialPara,
    sga-patt.global.attributes,
    sga-patt.placement.attributes,
    sga-patt.pointing.attributes,
    sga-patt.typed.attributes,
    
    ## indicates whether the copy text shows the exact place of reference for the note.
    [ a:defaultValue = "true" ]
    attribute anchored { sga-pdata.truthValue }?,
    
    ## points to the end of the span to which the note is attached, if the note is not embedded in the text at that point.
    attribute targetEnd {
      list { sga-pdata.pointer+ }
    }?,
    empty
  }
sga-pindex =
  
  ## (index entry) marks a location to be indexed for whatever purpose. [3.8.2. ]
  element index {
    (sga-pterm, sga-pindex?)*,
    sga-patt.global.attributes,
    sga-patt.spanning.attributes,
    
    ## a single word which follows the rules defining a legal XML name (see ), supplying a name to specify which index (of several) the index entry belongs to.
    attribute indexName { sga-pdata.name }?,
    empty
  }
sga-pmedia =
  
  ## indicates the location of any form of external media such as an audio or video clip etc. [3.9. ]
  element media {
    sga-pmodel.descLike*,
    sga-patt.typed.attributes,
    sga-patt.global.attributes,
    sga-patt.media.attribute.width,
    sga-patt.media.attribute.height,
    sga-patt.media.attribute.scale,
    sga-patt.resourced.attributes,
    sga-patt.timed.attributes,
    
    ## (MIME media type) specifies the applicable multimedia internet mail extension (MIME) media type
    attribute mimeType {
      list { sga-pdata.word+ }
    },
    empty
  }
sga-pgraphic =
  
  ## indicates the location of an inline graphic, illustration, or figure. [3.9. ]
  element graphic {
    sga-pmodel.descLike*,
    sga-patt.global.attributes,
    sga-patt.media.attributes,
    sga-patt.resourced.attributes,
    empty
  }
sga-pmilestone =
  
  ## marks a boundary point separating any kind of section of a text, typically but not necessarily indicating a point at which some part of a standard reference system changes, where the change is not represented by a structural element. [3.10.3. ]
  element milestone {
    empty,
    sga-patt.global.attributes,
    sga-patt.typed.attributes,
    sga-patt.edition.attributes,
    sga-patt.spanning.attributes,
    sga-patt.breaking.attributes,
    
    ##
    attribute unit {
      
      ##
      "tei:div"
      | 
        ##
        "tei:p"
      | 
        ##
        "tei:lg"
      | 
        ##
        "tei:date"
      | 
        ##
        "tei:speaker"
      | 
        ##
        "tei:q"
      | 
        ##
        "tei:stage"
      | 
        ##
        "tei:seg"
      | 
        ##
        "tei:note"
    },
    empty
  }
sga-prespStmt =
  
  ## (statement of responsibility) supplies a statement of responsibility for the intellectual content of a text, edition, recording, or series, where the specialized elements for authors, editors, etc. do not suffice or do not apply. May also be used to encode information about individuals or organizations which have played a role in the production or distribution of a bibliographic work. [3.11.2.2.  2.2.1.  2.2.2.  2.2.5. ]
  element respStmt {
    ((sga-presp+, sga-pmodel.nameLike.agent+)
     | (sga-pmodel.nameLike.agent+, sga-presp+)),
    sga-patt.global.attributes,
    empty
  }
sga-presp =
  
  ## (responsibility) contains a phrase describing the nature of a person's intellectual responsibility, or an organization's role in the production or distribution of a work. [3.11.2.2.  2.2.1.  2.2.2.  2.2.5. ]
  element resp {
    sga-pmacro.phraseSeq.limited,
    sga-patt.global.attributes,
    sga-patt.canonical.attributes,
    empty
  }
sga-ptitle =
  
  ## contains a title for any kind of work. [3.11.2.2.  2.2.1.  2.2.5. ]
  element title {
    sga-pmacro.paraContent,
    sga-patt.global.attributes,
    sga-patt.canonical.attributes,
    sga-patt.typed.attribute.subtype,
    
    ## classifies the title according to some convenient typology.
    ## Sample values include: 1] main; 2] sub(subordinate) ; 3] alt(alternate) ; 4] short; 5] desc(descriptive) 
    attribute type { sga-pdata.enumerated }?,
    
    ## indicates the bibliographic level for a title, that is, whether it identifies an article, book, journal, series, or unpublished material.
    attribute level {
      
      ## (analytic) analytic title (article, poem, or other item published as part of a larger item) 
      "a"
      | 
        ## (monographic) monographic title (book, collection, or other item published as a distinct item, including single volumes of multi-volume works) 
        "m"
      | 
        ## (journal) journal title 
        "j"
      | 
        ## (series) series title 
        "s"
      | 
        ## (unpublished) title of unpublished material (including theses and dissertations unless published by a commercial press)
        "u"
    }?,
    empty
  }
sga-pcitedRange =
  
  ##  (cited range) defines the range of cited content, often represented by pages or other units [3.11.2.3. ]
  element citedRange {
    sga-pmacro.phraseSeq,
    sga-patt.global.attributes,
    sga-patt.pointing.attributes,
    
    ## identifies the type of information conveyed by the element, e.g. columns, pages, volume.
    ## Suggested values include: 1] vol (volume) ; 2] issue; 3] pp (pages) ; 4] ll (lines) ; 5] chap (chapter) ; 6] part
    attribute unit {
      
      ##  (volume) the element contains a volume number.
      "vol"
      | 
        ## the element contains an issue number, or volume and issue numbers.
        "issue"
      | 
        ##  (pages) the element contains a page number or page range.
        "pp"
      | 
        ##  (lines) the element contains a line number or line range.
        "ll"
      | 
        ##  (chapter) the element contains a chapter indication (number and/or title)
        "chap"
      | 
        ## the element identifies a part of a book or collection.
        "part"
      | xsd:Name
    }?,
    
    ## specifies the starting point of the range of units indicated by the unit attribute.
    attribute from { sga-pdata.word }?,
    
    ## specifies the end-point of the range of units indicated by the unit attribute.
    attribute to { sga-pdata.word }?,
    empty
  }
sga-pbibl =
  
  ## (bibliographic citation) contains a loosely-structured bibliographic citation of which the sub-components may or may not be explicitly tagged.  [3.11.1.  2.2.7.  15.3.2. ]
  element bibl {
    (text
     | sga-pmodel.gLike
     | sga-pmodel.highlighted
     | sga-pmodel.pPart.data
     | sga-pmodel.pPart.edit
     | sga-pmodel.segLike
     | sga-pmodel.ptrLike
     | sga-pmodel.biblPart
     | sga-pmodel.global)*,
    sga-patt.global.attributes,
    sga-patt.declarable.attributes,
    sga-patt.typed.attributes,
    sga-patt.sortable.attributes,
    sga-patt.docStatus.attributes,
    empty
  }
sga-prelatedItem =
  
  ## contains or references some other bibliographic item which is related to the present one in some specified manner, for example as a constituent or alternative version of it. [3.11.2.6. ]
  element relatedItem {
    ((sga-pmodel.biblLike | sga-pmodel.ptrLike)?)
    >> sch:pattern [
         id = "relatedItem-constraint-targetorcontent11"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:relatedItem"
           "\x{a}" ~
           "               "
           sch:report [
             test = "@target and count( child::* ) > 0"
             "\x{a}" ~
             "If the @target attribute on "
             sch:name [ ]
             " is used, the\x{a}" ~
             "relatedItem element must be empty"
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ]
    >> sch:pattern [
         id = "relatedItem-constraint-targetorcontent11"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:relatedItem"
           "\x{a}" ~
           "               "
           sch:assert [
             test = "@target or child::*"
             "A relatedItem element should have either a 'target' attribute\x{a}" ~
             "        or a child element to indicate the related bibliographic item"
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ],
    sga-patt.global.attributes,
    sga-patt.typed.attributes,
    
    ## points to the related bibliographic element by means of an absolute or relative URI reference
    attribute target { sga-pdata.pointer }?,
    empty
  }
sga-pstage =
  
  ## (stage direction) contains any kind of stage direction within a dramatic text or fragment. [3.12.2.  3.12.  7.2.4. ]
  element stage {
    sga-pmacro.specialPara,
    sga-patt.global.attributes,
    
    ## indicates the kind of stage direction.
    ## Suggested values include: 1] setting; 2] entrance; 3] exit; 4] business; 5] novelistic; 6] delivery; 7] modifier; 8] location; 9] mixed
    attribute type {
      
      ## describes a setting.
      "setting"
      | 
        ## describes an entrance.
        "entrance"
      | 
        ## describes an exit.
        "exit"
      | 
        ## describes stage business.
        "business"
      | 
        ## is a narrative, motivating stage direction.
        "novelistic"
      | 
        ## describes how a character speaks.
        "delivery"
      | 
        ## gives some detail about a character.
        "modifier"
      | 
        ## describes a location.
        "location"
      | 
        ## more than one of the above
        "mixed"
      | xsd:Name
    }?,
    empty
  }
sga-pbody =
  
  ## (text body) contains the whole body of a single unitary text, excluding any front or back matter. [4. ]
  element body {
    (sga-pmodel.global*,
     (sga-pmodel.divTop, (sga-pmodel.global | sga-pmodel.divTop)*)?,
     (sga-pmodel.divGenLike,
      (sga-pmodel.global | sga-pmodel.divGenLike)*)?,
     ((sga-pmodel.divLike,
       (sga-pmodel.global | sga-pmodel.divGenLike)*)+
      | (sga-pmodel.div1Like,
         (sga-pmodel.global | sga-pmodel.divGenLike)*)+
      | ((sga-pmodel.common, sga-pmodel.global*)+,
         ((sga-pmodel.divLike,
           (sga-pmodel.global | sga-pmodel.divGenLike)*)+
          | (sga-pmodel.div1Like,
             (sga-pmodel.global | sga-pmodel.divGenLike)*)+)?)),
     (sga-pmodel.divBottom, sga-pmodel.global*)*),
    sga-patt.global.attributes,
    empty
  }
sga-pdiv =
  
  ## (text division) contains a subdivision of the front, body, or back of a text. [4.1. ]
  element div {
    ((sga-pmodel.divTop | sga-pmodel.global)*,
     ((((sga-pmodel.divLike | sga-pmodel.divGenLike),
        sga-pmodel.global*)+
       | ((sga-pmodel.common, sga-pmodel.global*)+,
          ((sga-pmodel.divLike | sga-pmodel.divGenLike),
           sga-pmodel.global*)*)),
      (sga-pmodel.divBottom, sga-pmodel.global*)*)?),
    sga-patt.global.attributes,
    sga-patt.divLike.attributes,
    sga-patt.typed.attributes,
    empty
  }
sga-ptrailer =
  
  ## contains a closing title or footer appearing at the end of a division of a text.  [4.2.4.  4.2. ]
  element trailer {
    sga-pmacro.phraseSeq,
    sga-patt.global.attributes,
    sga-patt.typed.attributes,
    empty
  }
sga-pdateline =
  
  ## contains a brief description of the place, date, time, etc. of production of a letter, newspaper story, or other work, prefixed or suffixed to it as a kind of heading or trailer.  [4.2.2. ]
  element dateline {
    sga-pmacro.phraseSeq, sga-patt.global.attributes, empty
  }
sga-popener =
  
  ## groups together dateline, byline, salutation, and similar phrases appearing as a preliminary group at the start of a division, especially of a letter. [4.2. ]
  element opener {
    (text
     | sga-pmodel.gLike
     | sga-pmodel.phrase
     | sga-pdateline
     | sga-psalute
     | sga-psigned
     | sga-pmodel.global)*,
    sga-patt.global.attributes,
    empty
  }
sga-pcloser =
  
  ## groups together salutations, datelines, and similar phrases appearing as a final group at the end of a division, especially of a letter. [4.2.2.  4.2. ]
  element closer {
    (text
     | sga-pmodel.gLike
     | sga-psigned
     | sga-pdateline
     | sga-psalute
     | sga-pmodel.phrase
     | sga-pmodel.global)*,
    sga-patt.global.attributes,
    empty
  }
sga-psalute =
  
  ## (salutation) contains a salutation or greeting prefixed to a foreword, dedicatory epistle, or other division of a text, or the salutation in the closing of a letter, preface, etc. [4.2.2. ]
  element salute {
    sga-pmacro.phraseSeq, sga-patt.global.attributes, empty
  }
sga-psigned =
  
  ## (signature) contains the closing salutation, etc., appended to a foreword, dedicatory epistle, or other division of a text.  [4.2.2. ]
  element signed {
    sga-pmacro.paraContent, sga-patt.global.attributes, empty
  }
sga-ppostscript =
  
  ## contains a postscript, e.g. to a letter. [4.2. ]
  element postscript {
    ((sga-pmodel.global | sga-pmodel.divTopPart)*,
     sga-pmodel.common,
     (sga-pmodel.global | sga-pmodel.common)*,
     (sga-pmodel.divBottomPart, sga-pmodel.global*)*),
    sga-patt.global.attributes,
    empty
  }
sga-pfacsimile =
  
  ## contains a representation of some written source in the form of a set of images rather than as transcribed or encoded text. [11.1. ]
  element facsimile {
    (sga-pmodel.graphicLike | sga-psurface | sga-psurfaceGrp)+,
    sga-patt.global.attributes,
    empty
  }
sga-psourceDoc =
  
  ## contains a transcription or other representation of a single source document potentially forming part of a dossier génétique or collection of sources.
  element sourceDoc {
    (sga-pmodel.global
     | sga-pmodel.graphicLike
     | sga-psurface
     | sga-psurfaceGrp)+,
    sga-patt.global.attributes,
    empty
  }
sga-patt.global.facs.attributes = sga-patt.global.facs.attribute.facs
sga-patt.global.facs.attribute.facs =
  
  ## (facsimile) points to all or part of an image which corresponds with the content of the element.
  attribute facs {
    list { sga-pdata.pointer+ }
  }?
sga-psurface =
  
  ## defines a written surface as a two-dimensional coordinate space, optionally grouping one or more graphic representations of that space, zones of interest within that space, and transcriptions of the writing within them.
  element surface {
    (sga-pmodel.glossLike*,
     sga-pmodel.graphicLike+,
     (sga-pzone | sga-pmodel.global | sga-pline)*)
    >> sch:ns [ prefix = "tei" uri = "http://www.tei-c.org/ns/1.0" ],
    sga-patt.global.attribute.el-target,
    sga-patt.global.attribute.att-target,
    sga-patt.global.attribute.att-value,
    sga-patt.global.attribute.rend,
    sga-patt.global.attribute.style,
    sga-patt.global.linking.attribute.next,
    sga-patt.global.linking.attribute.prev,
    sga-patt.global.facs.attribute.facs,
    sga-patt.coordinated.attribute.start,
    sga-patt.typed.attributes,
    
    ## (identifier) provides a unique identifier for the element bearing the attribute.
    attribute xml:id { xsd:ID },
    
    ## (number) gives a number (or other label) for an element, which is not necessarily unique within the document.
    attribute n { sga-pdata.text }?,
    
    ## (corresponds) Syntactic sugar attribute—should refer to the id of the sourceDoc element with which this surface should be associated
    attribute partOf {
      list { sga-pdata.pointer+ }
    },
    
    ## gives the x coordinate value for the upper left corner of a rectangular space.
    attribute ulx { sga-pdata.numeric },
    
    ## gives the y coordinate value for the upper left corner of a rectangular space.
    attribute uly { sga-pdata.numeric },
    
    ## gives the x coordinate value for the lower right corner of a rectangular space.
    attribute lrx { sga-pdata.numeric },
    
    ## gives the y coordinate value for the lower right corner of a rectangular space.
    attribute lry { sga-pdata.numeric },
    empty
  }
sga-psurfaceGrp =
  
  ## defines any kind of useful grouping of written surfaces, for example the recto and verso of a single leaf, which the encoder wishes to treat as a single unit. 
  element surfaceGrp {
    (sga-pmodel.global | sga-psurface | sga-psurfaceGrp)+,
    sga-patt.global.attributes,
    sga-patt.coordinated.attributes,
    sga-patt.typed.attributes,
    empty
  }
sga-patt.coordinated.attributes =
  sga-patt.coordinated.attribute.start,
  sga-patt.coordinated.attribute.ulx,
  sga-patt.coordinated.attribute.uly,
  sga-patt.coordinated.attribute.lrx,
  sga-patt.coordinated.attribute.lry
sga-patt.coordinated.attribute.start =
  
  ## indicates the element within a transcription of the text containing at least the start of the writing represented by this zone or surface.
  attribute start { sga-pdata.pointer }?
sga-patt.coordinated.attribute.ulx =
  
  ## gives the x coordinate value for the upper left corner of a rectangular space.
  attribute ulx { sga-pdata.numeric }?
sga-patt.coordinated.attribute.uly =
  
  ## gives the y coordinate value for the upper left corner of a rectangular space.
  attribute uly { sga-pdata.numeric }?
sga-patt.coordinated.attribute.lrx =
  
  ## gives the x coordinate value for the lower right corner of a rectangular space.
  attribute lrx { sga-pdata.numeric }?
sga-patt.coordinated.attribute.lry =
  
  ## gives the y coordinate value for the lower right corner of a rectangular space.
  attribute lry { sga-pdata.numeric }?
sga-pzone =
  
  ## defines any two-dimensional area within a surface element.
  element zone {
    (text
     | sga-pmodel.linePart
     | sga-pmodel.graphicLike
     | sga-pmodel.milestoneLike
     | sga-pmodel.noteLike
     | sga-pmodel.global.edit
     | sga-pmetamark)*,
    sga-patt.global.attributes,
    sga-patt.coordinated.attributes,
    sga-patt.typed.attribute.subtype,
    
    ##
    attribute type {
      
      ## Material that can appear at the top of a manuscript page—like headings or notes
      "top"
      | 
        ## Zone for marginal additions, deletions, and other modifications. More than one zone with this type may appear in a document if marginal zones are distinct interventions.
        "left_margin"
      | 
        ## The main block of text on a page
        "main"
      | 
        ## For segments of a page where page numbers have been added in one or more original hands; distinct from later additions by librarians/collectors
        "pagination"
      | 
        ## A zone of writing which appears to have been added by librarians/collectors at a later date—often page numbers
        "library"
      | 
        ## A zone of writing which is not part of the main text block and appears at the bottom of a page
        "bottom"
    },
    
    ## identifies a two dimensional area within the bounding box specified by the other attributes by means of a series of pairs of numbers, each of which gives the x,y coordinates of a point on a line enclosing the area.
    attribute points {
      list {

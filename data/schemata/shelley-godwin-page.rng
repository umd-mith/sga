            </zeroOrMore>
            <zeroOrMore>
               <group>
                  <ref name="sga-pmodel.divBottomPart"/>
               </group>
               <zeroOrMore>
                  <ref name="sga-pmodel.global"/>
               </zeroOrMore>
            </zeroOrMore>
         </group>
         <ref name="sga-patt.global.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="sga-pc">
      <element name="c">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(character) represents a character. [17.1. Linguistic Segment Categories]</a:documentation>
         <ref name="sga-pmacro.xtext"/>
         <ref name="sga-patt.global.attributes"/>
         <ref name="sga-patt.segLike.attributes"/>
         <ref name="sga-patt.typed.attributes"/>

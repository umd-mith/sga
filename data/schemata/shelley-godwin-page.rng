         <ref name="sga-patt.global.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="sga-patt.global.linking.attributes">
      <ref name="sga-patt.global.linking.attribute.corresp"/>
      <ref name="sga-patt.global.linking.attribute.next"/>
      <ref name="sga-patt.global.linking.attribute.prev"/>

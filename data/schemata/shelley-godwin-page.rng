   <define name="sga-pbody">
      <element name="body">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(text body) contains the whole body of a single unitary text, excluding any front or back matter. [4. Default Text Structure]</a:documentation>
         <group>
            <zeroOrMore>
               <ref name="sga-pmodel.global"/>
            </zeroOrMore>
            <optional>
               <group>
                  <group>
                     <ref name="sga-pmodel.divTop"/>
                  </group>
                  <zeroOrMore>
                     <choice>
                        <ref name="sga-pmodel.global"/>
                        <ref name="sga-pmodel.divTop"/>
                     </choice>
                  </zeroOrMore>
               </group>
            </optional>
            <optional>
               <group>
                  <group>
                     <ref name="sga-pmodel.divGenLike"/>
                  </group>
                  <zeroOrMore>
                     <choice>
                        <ref name="sga-pmodel.global"/>
                        <ref name="sga-pmodel.divGenLike"/>
                     </choice>
                  </zeroOrMore>
               </group>
            </optional>
            <group>
               <choice>
                  <oneOrMore>
                     <group>
                        <group>
                           <ref name="sga-pmodel.divLike"/>
                        </group>
                        <zeroOrMore>
                           <choice>
                              <ref name="sga-pmodel.global"/>
                              <ref name="sga-pmodel.divGenLike"/>
                           </choice>
                        </zeroOrMore>

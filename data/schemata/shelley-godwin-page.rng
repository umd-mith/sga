   <define name="sga-pglyph">
      <element name="glyph">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(character glyph) provides descriptive information about a character glyph. [5.2. Markup Constructs for Representation of Characters and Glyphs]</a:documentation>
         <group>
            <optional>
               <ref name="sga-pglyphName"/>
            </optional>
            <zeroOrMore>
               <ref name="sga-pmodel.descLike"/>
            </zeroOrMore>
            <zeroOrMore>
               <ref name="sga-pcharProp"/>
            </zeroOrMore>
            <zeroOrMore>
               <ref name="sga-pmapping"/>
            </zeroOrMore>
            <zeroOrMore>
               <ref name="sga-pfigure"/>
            </zeroOrMore>
            <zeroOrMore>
               <ref name="sga-pmodel.graphicLike"/>
            </zeroOrMore>
            <zeroOrMore>
               <ref name="sga-pmodel.noteLike"/>

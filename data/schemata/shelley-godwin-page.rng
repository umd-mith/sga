                  </oneOrMore>
                  <group>
                     <oneOrMore>
                        <group>
                           <ref name="sga-pmodel.common"/>
                        </group>
                        <zeroOrMore>
                           <ref name="sga-pmodel.global"/>
                        </zeroOrMore>
                     </oneOrMore>
                     <optional>
                        <choice>
                           <oneOrMore>
                              <group>
                                 <group>
                                    <ref name="sga-pmodel.divLike"/>
                                 </group>
                                 <zeroOrMore>
                                    <choice>
                                       <ref name="sga-pmodel.global"/>
                                       <ref name="sga-pmodel.divGenLike"/>
                                    </choice>
                                 </zeroOrMore>

         <ref name="sga-patt.global.attributes"/>
         <ref name="sga-patt.transcriptional.attributes"/>
         <ref name="sga-patt.typed.attribute.subtype"/>
         <ref name="sga-patt.spanning.attributes"/>
         <optional>
            <attribute name="type">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
               <choice>
                  <value>additions</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
                  <value>deletions</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
               </choice>
            </attribute>
         </optional>
         <empty/>
      </element>
   </define>
   <define name="sga-pretrace">

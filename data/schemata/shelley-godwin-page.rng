   <define name="sga-pdiv">
      <element name="div">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(text division) contains a subdivision of the front, body, or back of a text. [4.1. Divisions of the Body]</a:documentation>
         <group>
            <zeroOrMore>
               <choice>
                  <ref name="sga-pmodel.divTop"/>
                  <ref name="sga-pmodel.global"/>
               </choice>
            </zeroOrMore>
            <optional>
               <choice>
                  <group>
                     <oneOrMore>
                        <choice>
                           <ref name="sga-pmodel.divLike"/>
                           <ref name="sga-pmodel.divGenLike"/>
                        </choice>
                        <zeroOrMore>
                           <ref name="sga-pmodel.global"/>
                        </zeroOrMore>
                     </oneOrMore>
                  </group>
                  <group>
                     <oneOrMore>
                        <group>
                           <ref name="sga-pmodel.common"/>
                        </group>
                        <zeroOrMore>
                           <ref name="sga-pmodel.global"/>
                        </zeroOrMore>
                     </oneOrMore>
                     <zeroOrMore>
                        <choice>
                           <ref name="sga-pmodel.divLike"/>
                           <ref name="sga-pmodel.divGenLike"/>
                        </choice>
                        <zeroOrMore>
                           <ref name="sga-pmodel.global"/>
                        </zeroOrMore>
                     </zeroOrMore>
                  </group>
               </choice>
               <zeroOrMore>
                  <group>
                     <ref name="sga-pmodel.divBottom"/>
                  </group>
                  <zeroOrMore>
                     <ref name="sga-pmodel.global"/>

         <ref name="sga-pmacro.phraseSeq"/>
         <ref name="sga-patt.global.attributes"/>
         <ref name="sga-patt.naming.attributes"/>
         <ref name="sga-patt.typed.attributes"/>
         <ref name="sga-patt.source.attributes"/>

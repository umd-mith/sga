         <ref name="sga-patt.global.attribute.att-target"/>
         <ref name="sga-patt.global.attribute.att-value"/>
         <ref name="sga-patt.global.attribute.xmllang"/>
         <ref name="sga-patt.global.attribute.xmlbase"/>
         <ref name="sga-patt.global.attribute.xmlspace"/>
         <ref name="sga-patt.global.rendition.attribute.rend"/>
         <ref name="sga-patt.global.rendition.attribute.style"/>
